#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/Development/workspace-solop-docs/docs-vp/node_modules/.pnpm/esbuild@0.27.1/node_modules/esbuild/bin/node_modules:/opt/Development/workspace-solop-docs/docs-vp/node_modules/.pnpm/esbuild@0.27.1/node_modules/esbuild/node_modules:/opt/Development/workspace-solop-docs/docs-vp/node_modules/.pnpm/esbuild@0.27.1/node_modules:/opt/Development/workspace-solop-docs/docs-vp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/opt/Development/workspace-solop-docs/docs-vp/node_modules/.pnpm/esbuild@0.27.1/node_modules/esbuild/bin/node_modules:/opt/Development/workspace-solop-docs/docs-vp/node_modules/.pnpm/esbuild@0.27.1/node_modules/esbuild/node_modules:/opt/Development/workspace-solop-docs/docs-vp/node_modules/.pnpm/esbuild@0.27.1/node_modules:/opt/Development/workspace-solop-docs/docs-vp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../../../../esbuild@0.27.1/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
